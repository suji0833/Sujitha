class Employee{
	public void details(){
		System.out.println("Name"+"\t Year of joining"+"\tAddress");
		System.out.println("Robert"+"\t\t1994"+"\t\t64c-WallsStreet");
		System.out.println("Sam"+"\t\t2000"+"\t\t68D-WallsStreet");
		System.out.println("john"+"\t\t1999"+"\t\t26B-WallsStreet");
		
	}
}
public class details {

	public static void main(String[] args) {
	Employee emp1=new Employee();
	emp1.details();
	}
}
